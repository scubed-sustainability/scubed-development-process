{
  "S-cubed Project Config": {
    "prefix": "scubed-config",
    "body": [
      "{",
      "  \"name\": \"${1:project-name}\",",
      "  \"version\": \"${2:1.0.0}\",",
      "  \"description\": \"${3:Project description}\",",
      "  \"created\": \"${CURRENT_DATE}\",",
      "  \"createdBy\": \"${4:Your name}\",",
      "  \"team\": [",
      "    {",
      "      \"name\": \"${5:Team member name}\",",
      "      \"role\": \"${6:Role}\",",
      "      \"email\": \"${7:email@company.com}\"",
      "    }",
      "  ],",
      "  \"phases\": {",
      "    \"discovery\": {",
      "      \"status\": \"${8:not-started|in-progress|completed}\",",
      "      \"startDate\": \"${9:YYYY-MM-DD}\",",
      "      \"targetDate\": \"${10:YYYY-MM-DD}\"",
      "    },",
      "    \"requirements\": {",
      "      \"status\": \"not-started\",",
      "      \"startDate\": null,",
      "      \"targetDate\": null",
      "    },",
      "    \"design\": {",
      "      \"status\": \"not-started\",",
      "      \"startDate\": null,",
      "      \"targetDate\": null",
      "    },",
      "    \"development\": {",
      "      \"status\": \"not-started\",",
      "      \"startDate\": null,",
      "      \"targetDate\": null",
      "    }",
      "  },",
      "  \"technologies\": {",
      "    \"frontend\": [${11:\"React\", \"TypeScript\"}],",
      "    \"backend\": [${12:\"Node.js\", \"Express\"}],",
      "    \"database\": [${13:\"PostgreSQL\"}],",
      "    \"cloud\": [${14:\"Azure\"}]",
      "  },",
      "  \"integrations\": {",
      "    \"claude\": {",
      "      \"enabled\": ${15:true},",
      "      \"model\": \"${16:claude-3-sonnet}\"",
      "    },",
      "    \"loop\": {",
      "      \"enabled\": ${17:true},",
      "      \"workspaceId\": \"${18:workspace-id}\"",
      "    },",
      "    \"github\": {",
      "      \"repository\": \"${19:org/repo-name}\",",
      "      \"branch\": \"${20:main}\"",
      "    }",

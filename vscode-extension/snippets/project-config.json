  "Requirements Template": {
    "prefix": "requirements-doc",
    "body": [
      "# ${1:Feature Name} Requirements",
      "",
      "**Document Version:** ${2:1.0}",
      "**Last Updated:** ${CURRENT_DATE}",
      "**Author:** ${3:Your name}",
      "**Reviewers:** ${4:Reviewer names}",
      "",
      "## Overview",
      "${5:Brief description of the feature or requirement}",
      "",
      "## Business Requirements",
      "### Objectives",
      "- ${6:Business objective 1}",
      "- ${7:Business objective 2}",
      "",
      "### Success Criteria",
      "- ${8:Success criteria 1}",
      "- ${9:Success criteria 2}",
      "",
      "## Functional Requirements",
      "### Core Features",
      "1. **${10:Feature 1}**",
      "   - ${11:Feature 1 description}",
      "   - Acceptance Criteria: ${12:Criteria}",
      "",
      "2. **${13:Feature 2}**",
      "   - ${14:Feature 2 description}",
      "   - Acceptance Criteria: ${15:Criteria}",
      "",
      "## Non-Functional Requirements",
      "- **Performance:** ${16:Performance requirements}",
      "- **Security:** ${17:Security requirements}",
      "- **Scalability:** ${18:Scalability requirements}",
      "- **Availability:** ${19:Availability requirements}",
      "",
      "## Technical Constraints",
      "- ${20:Constraint 1}",
      "- ${21:Constraint 2}",
      "",
      "## Dependencies",
      "- ${22:Dependency 1}",
      "- ${23:Dependency 2}",
      "",
      "## Risks and Assumptions",
      "### Risks",
      "- ${24:Risk 1}",
      "- ${25:Risk 2}",
      "",
      "### Assumptions",
      "- ${26:Assumption 1}",
      "- ${27:Assumption 2}",
      "",
      "## Approval",
      "- [ ] Business Stakeholder: ${28:Name}",
      "- [ ] Technical Lead: ${29:Name}",
      "- [ ] Product Owner: ${30:Name}"
    ],
    "description": "Comprehensive requirements document template"
  }
}